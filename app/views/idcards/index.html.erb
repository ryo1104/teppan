<div class="page-body-container">
  <div class="row">
    <div class="col s12 m9 user-page">
      <h1 class="h1_class">身分証明書登録</h1>
      <ul class="collection z-depth-1 user-page">
        <% @idcards.each do |idcard| %>
          <% if idcard.frontback == "front" %>
            表面：
          <% else %>
            裏面：
          <% end %>
          <%= idcard.image.filename %>&nbsp;&nbsp;
          <%= link_to '削除', idcard_path(idcard.id), :method => :delete, :class => "button-points" %><br><br>
        <% end %>
      </ul>
    </div>
  </div>
</div>