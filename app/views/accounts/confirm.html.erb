<div class="page-body-container">
  <div class="row">
    <div class="col s12 m9 user-page">
      <h1>お客様情報の確認</h1>
      <div class="collection z-depth-0 account-form">
        <%= form_with url: user_accounts_path(@user.id), local: true, method: :post do |f| %>
          <%= @account %>
          <div class="col s12 m12" id="account-form-field-row">
            <div class="col s12 m12 account-form">
              <%= f.submit '入力画面に戻る', name: 'back', class: "btn btn-primary withripple custom" %>
            </div>
          </div>
          <div class="col s12 m12" id="account-form-field-row">
            <div class="col s12 m12 account-form">
              <%= f.submit '保存する', data: {disable_with: '保存中...'}, class: "btn btn-primary withripple custom" %>
            </div>
          </div>
        <% end %>
      </div>
    </div>
  </div>
</div>