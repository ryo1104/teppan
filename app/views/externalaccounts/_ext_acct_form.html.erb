<div class="col s12 m12" id="account-form-field-row">
  <div class="col s12 m5 account-form-field">
    <%= label_tag '銀行名' %>
    <%= f.text_field :bank_name, :value => @bank_info[1]["bank_name"], :id => 'bank_name_input' %>
  </div>
  <div class="col s12 m7 account-form-field">
    <%= label_tag '支店名' %>
    <%= f.text_field :branch_name, :value => @bank_info[1]["branch_name"], :id => 'branch_name_input' %>
  </div>
</div>
<div class="col s12 m12" id="account-form-field-row">
  <div class="col s12 m5 account-form-field">
    <%= label_tag '口座番号' %>
    <%= f.text_field :account_number, :value => @bank_info[1]["account_number"] %>
  </div>
</div>
<div class="col s12 m12" id="account-form-field-row">
  <div class="col s12 m5 account-form-field">
    <%= label_tag '口座名義人' %>
    <%= f.text_field :account_holder_name, :value => @bank_info[1]["account_holder_name"] %>
  </div>
</div>