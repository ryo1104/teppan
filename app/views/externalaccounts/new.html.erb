<% breadcrumb :new_externalaccount, @account %>
<div class="page-body-container">
  <p><%= breadcrumbs separator: "  &rsaquo;  ", :class => "breadcrumbs custom" %></p>
  <div class="row">
    <div class="col s12 m9 user-page">
      <h1 class="h1_class">銀行口座登録</h1>
      <div class="collection z-depth-0 account-form">
        <%= form_with model: [@account, @ext_acct], local: true, method: :post do |form| %>
          <%= render :partial => "ext_acct_form", locals: { f: form } %>
          <%= form.submit "登録",data: {disable_with: '送信中...'}, class: "btn btn-primary withripple custom" %>
        <% end %>
      </div>
    </div>
  </div>
</div>