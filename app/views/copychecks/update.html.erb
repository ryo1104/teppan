<div class="row">
  <div class="col s12 m9 offset-m1">
    <h1 class="h1_class">コピーチェック</h1>
    <ul class="collection z-depth-1">
      <li class="collection-item">
        <% if @ccd_error_code.present? %>
          エラー：<%= @ccd_error_code %>, <%= @ccd_error_message %><br>
          <% if @ccd_error_validation.present? %>
            バリデーション：<%= @ccd_error_validation %>
          <% else %>
            送信したQueue ID：<%= @check_obj.queue_id %><br><br>
          <% end %>
        <% end %>
        <% if @ccd_detail_error_code.present? %>
          詳細結果取得エラー：<%= @ccd_detail_error_code %>, <%= @ccd_detail_error_message %><br>
        <% end %>
      </li>
    </ul>
  </div>
</div>