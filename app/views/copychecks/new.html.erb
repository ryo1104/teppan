<div class="row">
  <div class="col s12 m9 offset-m1">
    <h1 class="h1_class">コピーチェック フォーム</h1>
    <ul class="collection z-depth-1">
      <li class="collection-item">
        <%= form_for @check_obj do |f| %>
          <p>本文：</p>
          <%= f.text_area :text, placeholder: "内容を書いて下さい（８００字以内）", style: 'width: 100%; height: 200px;', :maxlength => '800' %>
          <%= button_tag :type => "submit", class: "btn btn-primary withripple custom" do %>
            <i class="material-icons left">send</i>登録
          <% end %>
        <% end %>
      </li>
    </ul>
  </div>
</div>